generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Image {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  code       String
  number     Int
  url        String
  folder     String
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt

  @@map("images")
} 